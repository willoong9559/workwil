<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.wilwork.web.loginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>mainServlet</servlet-name>
        <servlet-class>com.wilwork.web.mainServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>mainServlet</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>qianServlet</servlet-name>
        <servlet-class>com.wilwork.web.qianServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>qianServlet</servlet-name>
        <url-pattern>/qian</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login_yuanServlet</servlet-name>
        <servlet-class>com.wilwork.web.login_yuanServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login_yuanServlet</servlet-name>
        <url-pattern>/y_login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>y_qianServlet</servlet-name>
        <servlet-class>com.wilwork.web.y_qianServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>y_qianServlet</servlet-name>
        <url-pattern>/y_qian</url-pattern>
    </servlet-mapping>

</web-app>